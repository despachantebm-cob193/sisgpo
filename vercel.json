{
  "rewrites": [
    {
      "source": "/app/:path*",
      "destination": "/index.html" 
    }
  ],
  "builds": [
    {
      "src": "sisgpo-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "outputDirectory": "sisgpo-frontend/dist"
      }
    },
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ]
}